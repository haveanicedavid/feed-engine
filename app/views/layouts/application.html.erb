<!DOCTYPE html>
<html>
<head>
  <title>FeedEngine</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  
<% flash.each do |name, msg| %>
  <%= content_tag :div, msg, :class => "flash-#{name} flash " %>
<% end %>

<%= render :partial => 'layouts/nav.html.erb' %>
<%= yield %>
<%#= render :partial => 'layouts/footer.html.erb' %>
</body>
</html>
